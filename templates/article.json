{
  "sections": {
    "blog-post-banner": {
      "type": "blog-post-banner",
      "custom_css": [
        "img {max-height: 500px;}"
      ],
      "settings": {
        "show_category": true,
        "blog_header_background": "#f5f5f5",
        "blog_header_text_color": "#282828"
      }
    },
    "main": {
      "type": "main-article-anleitung",
      "settings": {
        "show_on_scroll_navigation": true,
        "show_author": false,
        "show_tags": true,
        "show_date": false,
        "show_share_buttons": false,
        "show_content": false,
        "show_steps": false,
        "show_faqs": false
      }
    },
    "blog-post-prev-next": {
      "type": "blog-post-prev-next",
      "settings": {
      }
    },
    "blog-post-comments": {
      "type": "blog-post-comments",
      "settings": {
        "show_gravatar": true
      }
    }
  },
  "order": [
    "blog-post-banner",
    "main",
    "blog-post-prev-next",
    "blog-post-comments"
  ]
}
